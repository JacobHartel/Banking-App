@using GiantNationalBankClient.Models 

@model GetUserResponseModel

<h2>List of Users</h2>

<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>User Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone Number</th>
            
        </tr>
    </thead>
    <tbody>
@foreach (var user in Model.User){
    <tr>
        <td>@user.UserId</td>
        <td>@user.FirstName</td>
        <td>@user.LastName</td>
        <td>@user.Phone</td>

    </tr>
}
    </tbody>
</table>